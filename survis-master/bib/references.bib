@inproceedings{Firat2023MoneyVis,
  author    = {Elif E. Firat and Dharmateja Vytla and Navya Vasudeva Singh and Zhuoqun Jiang and Robert S. Laramee},
  title     = {{MoneyVis: Open Bank Transaction Data for Visualization and Beyond}},
  booktitle = {EuroVis (Short Papers)},
  pages     = {109--113},
  year      = {2023},
  series    = {EuroVis}
  doi       = {10.2312/evs.20231052},
  url       = {https://diglib.eg.org/bitstreams/e12b0747-cf14-486a-b98e-1567bbef3199/download},
  keywords = {type:dataset, financial data, bank transactions, privacy, visualization, case study},
  abstract  = {MoneyVis presents the first open bank transaction dataset derived from anonymized retail customer data, spanning seven years and encompassing over 6,500 transactions. This dataset addresses the scarcity of accessible financial transaction data due to privacy concerns, enabling researchers to explore visualization techniques in financial domains. The study introduces visualization tools to analyze user financial behaviors and uncover hidden trading trends within the data.}
}

@inproceedings{Hullman2013Contextifier,
  author    = {Jessica Hullman and Nicholas Diakopoulos and Eytan Adar},
  title     = {{Contextifier: Automatic Generation of Annotated Stock Visualizations}},
  booktitle = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
  pages     = {2707--2716},
  year      = {2013},
  series    = {SIGCHI Conference},
  doi       = {10.1145/2470654.2481374},
  url       = {https://dl.acm.org/doi/10.1145/2470654.2481374},
  keywords  = {type:application, stock data, annotation, news integration, automated visualization, time series},
  abstract  = {Contextifier is a system that automatically produces annotated visualizations of stock behavior based on news articles about companies. By linking news events to stock price movements, it aids users in understanding the reasons behind stock fluctuations. The system's annotation algorithms are informed by studies of professionally created visualizations, considering visual salience, contextual relevance, and key events in a company's history.}
}

